*** OPEN THE NTUPLE AND FILL THE HISTOGRAMS ***
do i = 0, 99
	hi/fil 1 ./condor_data/111903-2045/hbook/bb-nt-[i].hbook 0 -x

	cdir //lun1

	nt/loop 10 bb_hists.f

	for/clos 1
enddo
do i = 0, 999
	hi/fil 1 ./condor_data/112003-0035/hbook/bb-nt-[i].hbook 0 -x

	cdir //lun1

	nt/loop 10 bb_hists.f

	for/clos 1
enddo
*** MAKE POSTSCRIPT OUTPUT ***
*opt stat 
*set stat 110011

*do i = 1,6
*
*	zone 2 3
*
*	hi/pl [i]000
*	hi/pl [i]020
*	hi/pl [i]030
*	hi/pl [i]040
*	hi/pl [i]050
*	hi/pl [i]060
*
*	exe postscript ./psout/bb-[i]a
*
*	zone 2 3
*
*	hi/pl [i]070
*	hi/pl [i]080
*	hi/pl [i]090
*	hi/pl [i]100
*	hi/pl [i]110
*	hi/pl [i]120
*
*	exe postscript ./psout/bb-[i]b
*enddo
*
*	zone 2 3
*
*	hi/pl 7000
*	hi/pl 7010
*	hi/pl 7020
*	hi/pl 7030
*	hi/pl 7040
*	hi/pl 7050
*	
*	exe postscript ./psout/bb-7a
*
*	hi/pl 7060
*	hi/pl 7070
*	hi/pl 7080
*	hi/pl 7110
*	hi/pl 7120
*	hi/pl 7130
*
*	exe postscript ./psout/bb-7b
*
hi/op/divide 8010 8000 8001
hi/op/divide 8030 8020 8021
hi/op/divide 8050 8040 8041
hi/op/divide 8070 8060 8061

hi/op/subtract 8010 8000 8002
hi/op/subtract 8030 8020 8022
hi/op/subtract 8050 8040 8042
hi/op/subtract 8070 8060 8062

zone 2 2

HI/2D/surf 8000 270 0 2
HI/2d/surf 8010 270 0 2
hi/2d/surf 8001 270 0 2
hi/2d/surf 8002 270 0 2
exe postscript ./psout/bb-8a
