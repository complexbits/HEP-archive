(100) # (      h2->Fill\(lifetime[ja]\);) N
(101) # (      if \(tag[ja] == 1\){) N
(102) # () S 8 T (h3->Fill\(lifetime[ja]\);) N
(103) # (      }else if\(tag[ja] == -1\){) N
(104) # () S 8 T (h4->Fill\(lifetime[ja]\);) N
(105) # (      }else{) N
(106) # () S 8 T (cout << "Tag value "<<tag[ja]<< " out of range, should be -1 or 1" << en) N
0 T (dl;) N
(107) # () S 8 T (break;) N
(108) # (      }) N
(109) # (    }) N
(110) # () N
(111) # (    // Calculate \(weighted\) avg. nSigma for fitting) N
(112) # () N
(113) # (    Int_t bweight = 0.;) N
(114) # (    Double_t nSigavg = 0.;) N
(115) # (    for\(Int_t jb=0.; jb<nBins; jb++\){) N
(116) # () N
(117) # (      bweight = h2->GetBinContent\(jb+1\);) N
(118) # (      nSigavg = nSigavg + bweight*\(nSigma*\(min+\(jb*\(max-min\)/nBins\)\)\);) N
(119) # (      ) N
(120) # (    }) N
(121) # (    nSigavg = nSigavg/nEvts;) N
(122) # (    oscpar2_init = tSigma + nSigavg;    ) N
(123) # (    cout << "nSigavg = "<< nSigavg << ", oscpar2_init = " << oscpar2_init << end) N
0 T (l;   ) N
(124) # (    ) N
(125) # (    ) N
(126) # (    // Do unbinned likelihood fit) N
(127) # (    ) N
